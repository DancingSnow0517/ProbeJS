{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "typeRoots": [
      "probe"
    ],
    "baseUrl": ".",
    "paths": {
      "@java/*": [
        "probe/java/*"
      ],
      "@scripts/*": [
        "probe/scripts/*"
      ],
      "@kubejs/eventgroups": [
        "probe/events.d.ts"
      ]
    },
    "lib": [
      "ES2015",
      "ES5"
    ]
  }
}